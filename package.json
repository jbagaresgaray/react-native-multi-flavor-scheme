{
  "name": "reactnativemultiflavorscheme",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android-stg": "react-native run-android --variant=stgDebug --appIdSuffix=stg",
    "android-prod": "react-native run-android --variant=prodDebug",
    "build-dev": "cd android && ./gradlew assembleDevRelease",
    "build-stg": "cd android && ./gradlew assembleStgRelease",
    "build-prod": "cd android && ./gradlew assembleProdRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.67.1",
    "react-native-config": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
